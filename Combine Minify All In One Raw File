(()=>{const e=(e,t,o,n,c)=>{const d=document.createElement("button");d.textContent=e,Object.assign(d.style,{position:"fixed",top:t,left:"120px",zIndex:"9999",backgroundColor:"#28a745",color:"white",padding:`${n} ${o}`,border:"none",borderRadius:"20px",cursor:"pointer"}),document.body.appendChild(d),d.addEventListener("click",(async()=>{try{if("paste"===c){const e=document.getElementById("chat-input");e&&(e.focus(),e.setRangeText(await navigator.clipboard.readText(),e.selectionStart,e.selectionEnd,"end"),e.dispatchEvent(new Event("input",{bubbles:!0})),e.dispatchEvent(new Event("change",{bubbles:!0})),setTimeout((()=>{e.dispatchEvent(new KeyboardEvent("keydown",{key:"Enter",keyCode:13,bubbles:!0})),e.dispatchEvent(new KeyboardEvent("keyup",{key:"Enter",keyCode:13,bubbles:!0}))}),100))}else if("copy"===c){const e=document.querySelectorAll("div.ds-markdown-code-copy-button");e.length&&e[e.length-1].click()}d.style.backgroundColor="white",d.style.color="paste"===c?"#28a745":"black",d.textContent="✔️",setTimeout((()=>{d.style.backgroundColor="#28a745",d.style.color="white",d.textContent=e}),2e3)}catch(e){console.error("Error:",e)}}))};e("Paste Clipboard","200px","160px","60px","paste"),e("Copy Me","400px","180px","60px","copy")})();
